@article{Filatova2015,
abstract = {Killer whale populations may differ in genetics, morphology, ecology, and behavior. In the North Pacific, two sympatric populations (“resident” and “transient”) specialize on different prey (fish and marine mammals) and retain reproductive isolation. In the eastern North Atlantic, whales from the same populations have been observed feeding on both fish and marine mammals. Fish-eating North Pacific “residents” are more genetically related to eastern North Atlantic killer whales than to sympatric mammal-eating “transients.” In this paper, a comparison of frequency variables in killer whale calls recorded from four North Pacific resident, two North Pacific transient, and two eastern North Atlantic populations is reported to assess which factors drive the large-scale changes in call structure. Both low-frequency and high-frequency components of North Pacific transient killer whale calls have significantly lower frequencies than those of the North Pacific resident and North Atlantic populations. The difference in frequencies could be related to ecological specialization or to the phylogenetic history of these populations. North Pacific transient killer whales may have genetically inherited predisposition toward lower frequencies that may shape their learned repertoires.V},
author = {Filatova, Olga A. and Miller, Patrick J. O. and Yurk, Harald and Samarra, Filipa I. P. and Hoyt, Erich and Ford, John K. B. and Matkin, Craig O. and Barrett-Lennard, Lance G.},
doi = {10.1121/1.4922704},
isbn = {1520-8524 (Electronic)0001-4966 (Linking)},
issn = {0001-4966},
journal = {The Journal of the Acoustical Society of America},
number = {1},
pages = {251--257},
pmid = {26233024},
title = {{Killer whale call frequency is similar across the oceans, but varies across sympatric ecotypes}},
url = {http://asa.scitation.org/doi/10.1121/1.4922704},
volume = {138},
year = {2015}
}
