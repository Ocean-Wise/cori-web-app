keywords = {Animal care,Aquaculture,Aquariums,Dolphins,Sound monitoring,Underwater noise},
pages = {455--459},
pmid = {26610991},
title = {{A Permanent Soundscape Monitoring System for the Care of Animals in Aquaria}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26610991 http://link.springer.com/10.1007/978-1-4939-2981-8{\_}54},
